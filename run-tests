#!/bin/bash
set -x
FLAGS="--no-warn-unreachable"
if [ "$#" -gt 0 ]; then
    for n in "$@"
    do
        . <(sed -n $(($n+9))p $0)
    done
else
java -jar libs/kawa.jar $FLAGS -f src/test-parser.scm
java -jar libs/kawa.jar $FLAGS -f src/test-painter.scm
java -jar libs/kawa.jar $FLAGS -f src/test-editor-operations.scm
fi
